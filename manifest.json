{
  "manifest_version": 3,
  "name": "ChatGPT Down & Up",
  "author": "Eun",
  "version": "1.0",
  "description": "Extracts Q&A content from ChatGPT and upload file to ChatGPT.",
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["./scripts/content.js"],
      "run_at": "document_end"
    }
  ]
}
