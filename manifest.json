{
  "manifest_version": 3,
  "name": "ChatterKeep",
  "author": "Eun",
  "version": "1.0",
  "description": "An extension that allows you to save conversation and upload files.",
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["./scripts/simsunb-normal.js", "./scripts/jspdf.umd.min.js", "./scripts/content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "./image/icon.png"
  }
}
